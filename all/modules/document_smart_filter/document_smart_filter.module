/**
 * Implementation of hook_block_info().
 */
function document_smart_filter_block_info() {
  $blocks['document_smart_filter'] = array(
    'info' => t('Document Smart Filter'),
  );
  return $blocks;
}

 /**
 * Implementation of hook_block_permission().
 */
 
function document_smart_filter_permission() {
  return array(
    'document_smart_filter' => array(
      'title' => t('Administer Document Smart Filter'),
      'description' => t('Perform administration tasks for Fund Centre.'),
	  'restrict access' => true,
    ),
  );
}


function document_smart_filter_help($path, $arg) {
  switch ($path) {
    case "admin/help#document_smart_filter":
      return '<p>' . t("Displays links to nodes created on this date") . '</p>';
      break;
  }
}


