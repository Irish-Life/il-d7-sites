function document_smart_filter_block_view($delta = '') {
  switch($delta) {
    case 'document_smart_filter':
    $block['subject'] = t('');
    if (user_access('access content')) {
      // Use our custom function to retrieve data
      $result = document_smart_filter_content();
      // No content in the last week

      $block['content'] = $result;
      drupal_add_css(drupal_get_path('module', 'document_smart_filter') . '/css/document_smart_filter.css', array('group' => CSS_DEFAULT));
      drupal_add_js(drupal_get_path('module', 'document_smart_filter') .'/js/document-smart-filter.js');


    }
    return $block;
  }
 }
 